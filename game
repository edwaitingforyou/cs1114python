def battleBetTwoTeam(strengthOfTeamOne, stengthOfTeamTwo):
    strengthOfTeamOne, strengOfTeamTwo = getStrengthOfWarriors
    res = strengthOfTeamOne - strengthOfTeamTwo
    if res > 0:
        winner = 1
        socer = res
        return winner,score
    if res < 0:
        winner = 2
        socer = 0-res
        return winner,score
